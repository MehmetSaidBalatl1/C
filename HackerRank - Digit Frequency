// HackerRank - Digit Frequency //
// Task
Given a string, s, consisting of alphabets and digits, find the frequency of each digit in the given string.
//

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char s[1000];
    scanf("%s", s);
    
    int number;
    for(number = 0; number < 10; number++) {
        int count = 0;
        int i = 0;
        while(s[i]) {
            if(s[i] == '0' + number) count++;
            i++;
        }
        printf("%d ", count);
    }
    return 0;
}
