// HackerRank - Playing With Characters //

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#define MAX 100

int main() 
{
    char ch;
    scanf("%c", &ch);
    char s[MAX];
    scanf("%s", s);
    char sen[MAX];
    scanf("\n");
    scanf("%[^\n]", sen);
    printf("%c\n%s\n%s\n",ch,s,sen);
    
    return 0;
}
