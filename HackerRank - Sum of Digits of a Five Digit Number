// HackerRank - Sum of Digits of a Five Digit Number //
// Task
Given a five digit integer, print the sum of its digits.
//
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int i,sum,n;
    scanf("%d", &n);
    for(i=0;i<5;i++){
    sum += n%10;
    n = n/10;
    }
    printf("%d",sum);
    return 0;
}
